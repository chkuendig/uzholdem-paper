@article{Gilpin2007,
	Abstract = {We present a computational approach to the saddle-point formulation for the Nash equilibria of two-person, zero-sum sequential\newline games of imperfect information. The algorithm is a first-order gradient method based on modern smoothing techniques for non-smooth\newline convex optimization. The algorithm requires O(1/e) iterations to compute an e-equilibrium, and the work per iteration is extremely low. These features enable us to find approximate Nash equilibria for\newline sequential games with a tree representation of about 1010 nodes. This is three orders of magnitude larger than what previous algorithms can handle. We present two heuristic improvements\newline to the basic algorithm and demonstrate their efficacy on a range of real-world games. Furthermore, we demonstrate how the\newline algorithm can be customized to a specific class of problems with enormous memory savings.},
	Author = {Gilpin, Andrew and Hoda, Samid and Pe\~na, Javier and Sandholm, Tuomas},
	Journal = {Internet and Network Economics},
	Pages = {57-69},
	Title = {{G}radient-{B}ased {A}lgorithms for {F}inding {N}ash {E}quilibria in {E}xtensive {F}orm {G}ames},
	Year = {2007}}

